<%- include('partials/header') %>

  <h1>Applications</h1>

  <table class="table table-bordered">
    <thead>
      <tr>
        <th>Internship</th>
        <th>Status</th>
      </tr>
    </thead>
    <tbody>
      <% applications.forEach(function(application) { %>
      <tr>
        <td><%= application.internship %></td>
        <td><%= application.status %></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
  <%- include('partials/footer') %>
